.container1 .card {
    max-width: 300px;
    height: 215px;
    margin: 30px 10px;
    padding: 20px 15px;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.5);
    transition: 0.3s ease-in-out
}

    .container1 .card:hover {
        height: 450px
    }

    .container1 .card .imgContainer {
        position: relative;
        width: 125px;
        height: 125px;
        top: -50px;
        left: 10px;
        z-index: 1;
        box-shadow: 0 5px 20px rgba(0, 0, 0, 0.2)
    }

        .container1 .card .imgContainer img {
            max-width: 100%;
            border-radius: 4px
        }

    .container1 .card .content1 {
        position: relative;
        margin-top: -70px;
        padding: 10px 15px;
        text-align: center;
        color: #111;
        visibility: hidden;
        opacity: 0;
        pointer-events: none;
        transition: 0.3s ease-in-out
    }

    .container1 .card:hover .content1 {
        visibility: visible;
        opacity: 1;
        margin-top: -40px;
        transition-delay: 0.3s;
    }


